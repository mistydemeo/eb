	sed -e 's;\@localedir\@;$(localedir);' \
	   $(srcdir)/build-post.h.in > build-post.h.tmp
	$(top_srcdir)/move-if-change build-post.h.tmp build-post.h
	@echo timestamp > stamp-build-post-h

